  },
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "start": "webpack serve --config webpack.config.js",
    "build": "webpack --config webpack.prod.js --mode production",
    "serve": "node server.js",
    "gh": "npx gh-pages -d dist/"
  },
  "repository": {
